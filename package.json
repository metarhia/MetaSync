{
  "name": "metasync",
  "version": "0.3.29",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Asynchronous Programming Library",
  "license": "MIT",
  "keywords": [
    "metasync",
    "callback",
    "promise",
    "async",
    "asyncronous",
    "parallel",
    "sequential",
    "metarhia",
    "flow",
    "collector",
    "errback",
    "err-first",
    "error-first",
    "callback-last",
    "throttle",
    "impress",
    "datacollector",
    "keycollector",
    "composition"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/metasync"
  },
  "main": "./metasync.js",
  "browser": {
    "./metasync.js": "./metasync.browser.js"
  },
  "readmeFilename": "README.md",
  "scripts": {
